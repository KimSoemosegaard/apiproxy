[% BLOCK download_button %]
  <button type="button" value="Download">
    <a href="[% href %]">
      <i class="fa fa-file-download"></i>
    </a>
  </button>
[% END %]

[% BLOCK print_button %]
  <button type="button" value="Print" onclick="window.print()">
